{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-console":"warn"
    }
}
